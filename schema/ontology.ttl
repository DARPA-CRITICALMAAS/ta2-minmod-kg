###  https://minmod.isi.edu/ontology/reference
:reference rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :mineral_inventory_objectproperty ;
           rdfs:range :Reference .


###  https://minmod.isi.edu/ontology/reference_objectproperty
:reference_objectproperty rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf owl:topObjectProperty ;
                          rdfs:domain :Reference .


###  https://minmod.isi.edu/ontology/source
:source rdf:type rdf:Property ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :MatchInfo
                                    :MineralSystem
                                  )
                    ] ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( xsd:string
                                   :MappableCriteria
                                 )
                   ] .


###  https://minmod.isi.edu/ontology/state_or_province
:state_or_province rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :location_info_objectproperty ;
                   rdfs:range :MatchInfo .


###  https://minmod.isi.edu/ontology/supporting_references
:supporting_references rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :mappable_criteria_objectproperty ;
                       rdfs:range :Reference .


###  https://minmod.isi.edu/ontology/trap
:trap rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :mineral_system_objectproperty ;
      rdfs:range :MappableCriteria .


###  https://minmod.isi.edu/ontology/unit
:unit rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :measure_objectproperty ;
      rdfs:range :MatchInfo .


