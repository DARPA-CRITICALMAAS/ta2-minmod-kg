           rdfs:subPropertyOf :reference_objectproperty ;
           rdfs:range :PageInfo .


###  https://minmod.isi.edu/ontology/page_info_objectproperty
:page_info_objectproperty rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf owl:topObjectProperty ;
                          rdfs:domain :PageInfo .


###  https://minmod.isi.edu/ontology/pathway
:pathway rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :mineral_system_objectproperty ;
         rdfs:range :MappableCriteria .


###  https://minmod.isi.edu/ontology/potential_dataset
:potential_dataset rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :mappable_criteria_objectproperty ;
