#################################################################
#    Classes
#################################################################

###  https://minmod.isi.edu/ontology/ThingHasLabel
:ThingHasLabel rdf:type owl:Class ;
               rdfs:comment "Instance of this class must have rdfs:label" .

###  https://minmod.isi.edu/ontology/BoundingBox
:BoundingBox rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :x_max ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :x_min ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :y_max ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :y_min ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] .


###  https://minmod.isi.edu/ontology/Commodity
:Commodity rdf:type owl:Class ;
           rdfs:subClassOf :ThingHasLabel .


###  https://minmod.isi.edu/ontology/CoordinateReferenceSystem
:CoordinateReferenceSystem rdf:type owl:Class ;
                           rdfs:subClassOf :ThingHasLabel .


###  https://minmod.isi.edu/ontology/Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf :ThingHasLabel .


###  https://minmod.isi.edu/ontology/DepositType
:DepositType rdf:type owl:Class ;
             rdfs:subClassOf :ThingHasLabel ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :environment ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :group ;
                               owl:cardinality "1"^^xsd:nonNegativeInteger
                             ] .


###  https://minmod.isi.edu/ontology/Document
:Document rdf:type owl:Class .


###  https://minmod.isi.edu/ontology/EvidenceLayer
:EvidenceLayer rdf:type owl:Class ;
               rdfs:subClassOf :ThingHasLabel ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :relevance_score ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] .


###  https://minmod.isi.edu/ontology/GeologyInfo
:GeologyInfo rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :age ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :comments ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :description ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :unit_name ;
                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                             ] .


###  https://minmod.isi.edu/ontology/LocationInfo
:LocationInfo rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :crs ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :country ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :state_or_province ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :location ;
                                owl:maxCardinality "1"^^xsd:nonNegativeInteger
                              ] .


###  https://minmod.isi.edu/ontology/MappableCriteria
:MappableCriteria rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :criteria ;
                                    owl:cardinality "1"^^xsd:nonNegativeInteger
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :theoretical ;
                                    owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                  ] .


###  https://minmod.isi.edu/ontology/MatchInfo
:MatchInfo rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :source ;
                             owl:allValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :source ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :normalized_uri ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :confidence ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :observed_name ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] .


###  https://minmod.isi.edu/ontology/Measure
:Measure rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :unit ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :value ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] .


###  https://minmod.isi.edu/ontology/MineralInventory
:MineralInventory rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :commodity ;
                                    owl:cardinality "1"^^xsd:nonNegativeInteger
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :reference ;
                                    owl:cardinality "1"^^xsd:nonNegativeInteger
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :cutoff_grade ;
                                    owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :grade ;
