@prefix : <https://minmod.isi.edu/ontology/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:BoundingBoxShape a sh:NodeShape ;
    sh:targetClass :BoundingBox .

:CommodityCandidateShape a sh:NodeShape ;
    sh:targetClass :CommodityCandidate .

:CommodityShape a sh:NodeShape ;
    sh:property [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:path rdfs:label ;
            sh:severity sh:Violation ] ;
    sh:targetClass :Commodity .

